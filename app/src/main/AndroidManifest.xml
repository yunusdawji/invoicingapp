<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.kloudportal.shop.quickbill" >
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="shopbillsample.retail.kloudportal.com.shopbillsample.application.ISInvoicingApplication"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:configChanges="keyboardHidden|orientation|screenSize"
        android:theme="@style/MyMaterialTheme"
        tools:replace="android:icon,android:theme"
        >
        <activity
            android:name="shopbillsample.retail.kloudportal.com.shopbillsample.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
